mysql> SELECT Subjects.name, AVG(Studies.mark) AS Media FROM Studies JOIN Subjects ON Subjects.id=Studies.subject_id WHERE Subjects.field="mat" AND Studies.mark>=5 GROUP BY Subjects.name;
