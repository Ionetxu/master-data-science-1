mysql> SELECT name, Sueldo FROM (SELECT teacher_id, 0.3*30*SUM(credits) AS Sueldo FROM (SELECT * FROM Teaches WHERE year=2017) AS t17 JOIN Subjects ON t17.subject_id=Subjects.id GROUP BY teacher_id) AS sueldo JOIN People ON sueldo.teacher_id=People.id;
