mysql> (SELECT teacher_id, subject_id, COUNT(teacher_id) AS profs FROM Teaches GROUP BY subject_id, teacher_id HAVING profs=3) AS mono INNER JOIN Subjects ON mono.subject_id=Subjects.id;
